{
  "name": "vill-v",
  "version": "1.3.1",
  "description": "开发中对于技术的总结，归纳",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "license": "MIT",
  "author": {
    "name": "whitekite",
    "email": "1075790909@qq.com"
  },
  "sideEffects": false,
  "homepage": "https://vill-v-kit.github.io/vill-v/",
  "repository": "https://github.com/vill-v-kit/vill-v.git",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./async": {
      "types": "./dist/async.d.ts",
      "import": "./dist/async.mjs",
      "require": "./dist/async.js"
    },
    "./broswer": {
      "types": "./dist/broswer.d.ts",
      "import": "./dist/broswer.mjs",
      "require": "./dist/broswer.js"
    },
    "./date": {
      "types": "./dist/date.d.ts",
      "import": "./dist/date.mjs",
      "require": "./dist/date.js"
    },
    "./path-prop": {
      "types": "./dist/path-prop.d.ts",
      "import": "./dist/path-prop.mjs",
      "require": "./dist/path-prop.js"
    },
    "./regexp-match": {
      "types": "./dist/regexp-match.d.ts",
      "import": "./dist/regexp-match.mjs",
      "require": "./dist/regexp-match.js"
    },
    "./ts": {
      "types": "./dist/ts.d.ts",
      "import": "./dist/ts.mjs",
      "require": "./dist/ts.js"
    },
    "./type-as": {
      "types": "./dist/type-as.d.ts",
      "import": "./dist/type-as.mjs",
      "require": "./dist/type-as.js"
    },
    "./vanilla": {
      "types": "./dist/vanilla.d.ts",
      "import": "./dist/vanilla.mjs",
      "require": "./dist/vanilla.js"
    }
  },
  "scripts": {
    "build": "pnpm dlx automd && tsup-node",
    "gen:file": "node ./build.mjs"
  },
  "dependencies": {
    "@vill-v/async": "workspace:*",
    "@vill-v/broswer": "workspace:*",
    "@vill-v/date": "workspace:*",
    "@vill-v/path-prop": "workspace:*",
    "@vill-v/regexp-match": "workspace:*",
    "@vill-v/ts": "workspace:*",
    "@vill-v/type-as": "workspace:*",
    "@vill-v/vanilla": "workspace:*"
  }
}
