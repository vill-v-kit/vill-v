{
  "name": "@vill-v/memo",
  "version": "1.3.0",
  "private": true,
  "homepage": "https://vill-v.gitee.io/",
  "repository": "https://gitee.com/vill-v/core",
  "packageManager": "pnpm@9.0.6",
  "scripts": {
    "build": "turbo run build",
    "release": "vbumpp -r",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "update:deps": "taze major -w -i -a -r -f",
    "test": "vitest --ui",
    "clean:deps": "vfdel node_modules packages/*/node_modules playground/*/node_modules",
    "force:clean:deps": "vfdel pnpm-lock.yaml && pnpm clean:deps",
    "force:install": "pnpm force:clean:deps && pnpm install"
  },
  "devDependencies": {
    "@types/node": "^20.12.8",
    "@vill-v/bumpp": "^1.0.4",
    "@vill-v/eslint-config": "^1.2.1",
    "@vill-v/prettier-config": "^1.0.0",
    "@vill-v/tsconfig": "^1.0.1",
    "@vitest/ui": "^1.5.3",
    "@vrx-cp/utils": "0.0.1-beta.97",
    "eslint": "^9.1.1",
    "globby": "^14.0.1",
    "prettier": "^3.2.5",
    "taze": "^0.13.8",
    "tsup": "^8.0.2",
    "turbo": "^1.13.3",
    "typescript": "^5.4.5",
    "unocss": "^0.59.4",
    "vitepress": "1.1.4",
    "vitest": "^1.5.3",
    "void-fs": "0.0.1-beta.15",
    "vue": "^3.4.26"
  }
}
