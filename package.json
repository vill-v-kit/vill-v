{
  "name": "@vill-v/memo",
  "version": "1.2.2",
  "private": true,
  "homepage": "https://vill-v.gitee.io/",
  "repository": "https://gitee.com/vill-v/core",
  "packageManager": "pnpm@7.27.0",
  "scripts": {
