  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.13.0",
    "@vill-v/bumpp": "^0.0.4",
    "@vill-v/eslint-config": "^0.0.11",
    "@vill-v/prettier-config": "^0.0.11",
    "@vill-v/tsconfig": "^0.0.11",
    "@vitest/ui": "^0.28.5",
    "eslint": "^8.34.0",
    "prettier": "^2.8.4",
    "taze": "^0.8.5",
    "tsup": "^6.6.2",
    "typescript": "^4.9.5",
    "vitepress": "^1.0.0-alpha.46",
    "vitest": "^0.28.5",
    "vue": "^3.2.47"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
